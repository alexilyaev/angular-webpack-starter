{
  "name": "angular-webpack-starter",
  "version": "0.1.0",
  "description": "A small example of how to do GitHub login using Express with Passport",
  "main": "app/app.js",
  "scripts": {
    "prestart": "npm install",
    "start": "webpack-dev-server --hot --progress",
    "prebuild": "npm prune; npm install",
    "build": "NODE_ENV=production webpack -p --progress",
    "prebuildwin": "npm prune&npm install",
    "buildwin": "set NODE_ENV=production&webpack -p --progress",
    "lint": "eslint -c .eslintrc-build ."
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:alexilyaev/express-github-login.git"
  },
  "license": "MIT",
  "dependencies": {
    "angular": "1.5.0",
    "angular-animate": "1.5.0",
    "angular-aria": "1.5.0",
    "angular-messages": "1.5.0",
    "angular-ui-router": "0.2.18",
    "lodash": "4.5.1",
    "normalize.css": "3.0.3",
    "open-sans-fontface": "1.4.0"
  },
  "devDependencies": {
    "autoprefixer": "6.3.3",
    "babel-core": "6.7.2",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-runtime": "6.6.0",
    "babel-preset-es2015": "6.6.0",
    "babel-runtime": "6.6.1",
    "clean-webpack-plugin": "0.1.8",
    "css-loader": "0.23.1",
    "eslint": "2.12.0",
    "eslint-config-angular": "0.5.0",
    "eslint-loader": "1.3.0",
    "eslint-plugin-angular": "1.0.0",
    "expose-loader": "0.7.1",
    "file-loader": "0.8.5",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.9.0",
    "imports-loader": "0.6.5",
    "json-loader": "0.5.4",
    "ng-annotate-loader": "0.1.0",
    "node-sass": "3.4.2",
    "postcss-loader": "0.8.1",
    "raw-loader": "0.5.1",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1"
  }
}
